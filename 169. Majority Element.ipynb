    "from collections import Counter\n",
    "\n",
    "class Solution:\n",
    "    def majorityElement(self, nums: List[int]) -> int:\n",
    "        cnt = Counter(nums)\n",
    "        # return max(cnt.keys(), key=cnt.get)\n",
    "        return max(cnt.keys(), key=lambda x: cnt[x])  # 这两个key都可以"
   ]
  }

   "source": [
    "class Solution:\n",
    "    def isUgly(self, num: int) -> bool:\n",
    "        if num < 1:\n",
    "            return False\n",
    "        for i in [2,3,5]:\n",
    "            while num % i == 0:\n",
    "                num //= i\n",
    "        return num == 1"
   ]

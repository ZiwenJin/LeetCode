    "- Easy\n",
    "- 转成str, 翻转, 转回int"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def reverse(self, x: int) -> int:\n",
    "        sign = 1 if x >= 0 else -1\n",
    "        s = str(abs(x))\n",
    "        x = sign * int(s[::-1])\n",
    "        return x if -2**31 <= x < 2**31 else 0"
   ]
  }
